<web-app>
	<display-name>Monitoring Application</display-name>

	<servlet>
		<servlet-name>index</servlet-name>
		<servlet-class>com.external.monitoring.web.Index</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>index</servlet-name>
		<url-pattern>/index</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>		
	</welcome-file-list>
</web-app>
